  "bugs": {
    "url": "https://github.com/aelbore/rollup-plugin-ngc/issues"
  },
  "homepage": "https://github.com/aelbore/rollup-plugin-ngc#readme",
  "peerDependencies": {
    "@angular/compiler-cli": "<=^9.1.11",
    "@angular-devkit/build-optimizer": "<=^0.901.8"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.901.8",
    "@angular/compiler": "^9.1.11",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/core": "^9.1.11",
    "aria-build": "^0.4.3",
    "aria-fs": "^0.4.3",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "typescript": "3.8.3"
